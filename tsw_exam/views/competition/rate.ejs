
<div id="content-panel" class="col-lg-12 col-md-12 col-sm-12">
    <div class="panel panel-default">
        <div class="panel panel-heading text-center">
            <h1>Lista zawodników</h1>
        </div>
        <form action="/competition/rate" method="post">
            <div class="panel panel-body">
            <% for(var i = 0;i<data.length;i++){ %>
                <div class="col-lg-10 col-lg-push-1 col-md-10 col-md-push-1 col-sm-12">
                    <h1 class="text-center">Zawodnik</h1>
                </div>
                <div class="row">
                    <div class="col-lg-2 col-md-3 col-sm-12">
                        <p>Głowa</p>
                    </div>
                    <div class="col-lg-8 col-md-7 col-sm-12">
                        
                    </div>
                </div>
                
                <div class="row">
                    <div class="col-lg-2 col-md-3 col-sm-12">
                        <p>Szyja</p>
                    </div>
                    <div class="col-lg-8 col-md-7 col-sm-12">
                        
                    </div>
                </div>
                
                <div class="row">
                    <div class="col-lg-2 col-md-3 col-sm-12">
                        <p>Kłoda</p>
                    </div>
                    <div class="col-lg-8 col-md-7 col-sm-12">
                        
                    </div>
                </div>
                
                <div class="row">
                    <div class="col-lg-2 col-md-3 col-sm-12">
                        <p>Nogi</p>
                    </div>
                    <div class="col-lg-8 col-md-7 col-sm-12">
                        
                    </div>
                </div>
                
                <div class="row">
                    <div class="col-lg-2 col-md-3 col-sm-12">
                        <p>Ruch</p>
                    </div>
                    <div class="col-lg-8 col-md-7 col-sm-12">
                        
                    </div>
                </div>
                <hr>
                <br>
            <% } %>
            </div>
            <div class="panel panel-footer">
                <button type="submit" >Prześlij oceny</button>
            </div>
        </form>
    </div>
</div>