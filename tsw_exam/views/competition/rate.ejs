
<div id="content-panel" class="col-lg-12 col-md-12 col-sm-12">
    <div class="panel panel-default">
        <div class="panel panel-heading text-center">
            <h1>Lista zawodników</h1>
        </div>
        <form class="form-horizontal" action="/competition/rate" method="post">
            <div class="panel panel-body">
            <% for(var i = 0;i<data.length;i++){ var k = 1;%>
                <div class="form-group">
                    <div class="col-lg-10 col-lg-push-1 col-md-10 col-md-push-1 col-sm-12">
                        <h1 style="color: white" class="text-center">Koń z numerem: <%= k %></h1>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-2 col-md-3 col-sm-12 control-label">Głowa</label>
                    <div class="col-lg-8 col-md-7 col-sm-12">

                    </div>
                </div>
                
                <div class="form-group">
                    <label class="col-lg-2 col-md-3 col-sm-12 control-label">Szyja</label>
                    <div class="col-lg-8 col-md-7 col-sm-12">
                        <input type="hidden" class="form-control single-slider" value="2"/>
                    </div>
                </div>
                
                <div class="form-group">
                    <label class="col-lg-2 col-md-3 col-sm-12 control-label">Kłoda</label>
                    <div class="col-lg-8 col-md-7 col-sm-12">
                    </div>
                </div>
                
                <div class="form-group">
                    <label class="col-lg-2 col-md-3 col-sm-12 control-label">Nogi</label>
                    <div class="col-lg-8 col-md-7 col-sm-12">
                    </div>
                </div>
                
                <div class="form-group">
                    <label class="col-lg-2 col-md-3 col-sm-12 control-label">Ruch</label>
                    <div class="col-lg-8 col-md-7 col-sm-12">
                    </div>
                </div>
                <hr>
                <br>
            <% k++; } %>
            </div>
            <div class="panel panel-footer">
                <button type="submit" >Prześlij oceny</button>
            </div>
        </form>
    </div>
</div>