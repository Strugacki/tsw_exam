
<div id="content-panel" class="col-lg-12 col-md-12 col-sm-12">
    <div class="panel panel-default">
        <div class="panel panel-heading text-center">
            <h1>Wyniki Zawodów</h1>
        </div>
        <div class="panel panel-body">
          <table class="table table-hover">
            <thead>
              <tr>
                <th>Nazwa zawodów</th>
                <th>Nazwa konia</th>
                <th>Właściciel konia</th>
                <th>Średnia ocena</th>
              </tr>
            </thead>
            <tbody>
             <% var i = 0; var j = 0; var k = 0; for(i=0; i < data.length; i++){ %>
              <tr>
                <td><%= data[i].competition.name %></td>
                <td><%= data[i].horse.horseName %></td>
                <td><%= data[i].horse.ownerFirstName %> <%= data[i].horse.ownerLastName %></td>
                <td><%= data[i].overall %></td>
              </tr>
             <% } %>
            </tbody>
          </table>
        </div>
    </div>
</div>